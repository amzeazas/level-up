<div class="container-fluid">
  <%= link_to "Edit Quest", edit_user_quest_path(@user, @quest), class:"btn btn-default pull-right", id:"edit-quest-button", remote: true %>
  <h1 id="quest-name"><%= @quest.name %></h1>
  <h4>
    <ul class="missions-list">
      <% @quest.missions.each do |mission| %>
        <li><%= mission.description %></li>
      <% end %>
    </ul>
  </h4>
</div>
